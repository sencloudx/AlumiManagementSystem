<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name="sencloud.sl.entity.Resume" table="resume" catalog="infor_manage">
        <id name="resumeId" type="java.lang.Integer">
            <column name="resume_id" />
            <generator class="identity"></generator>
        </id>
        <property name="startTM" type="java.lang.String">
            <column name="start_tm" length="20" />
        </property>
         <property name="endTM" type="java.lang.String">
            <column name="end_tm" length="20" />
        </property>
        <property name="resumeCompany" type="java.lang.String">
            <column name="resume_company" length="100" />
        </property>
        <property name="resumePost" type="java.lang.String">
            <column name="resume_post" length="50"/>
        </property>
        <property name="resumeDesc" type="java.lang.String">
            <column name="resume_desc"/>
        </property>
        <property name="userId" type="java.lang.Integer">
            <column name="user_id" length="100" />
        </property>
         <property name="stuNum" type="java.lang.String">
            <column name="stu_num" length="20" />
        </property>
    </class>
</hibernate-mapping>
